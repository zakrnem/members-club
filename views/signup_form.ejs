<%- include('./partials/head', {title: title}) %>
  <body>
    <h1><%= title %></h1>
    <a href="/">Back</a>
    <hr>
    <form method="POST" action>
      <div class="form-group">
        <label for="first_name">First name: </label>
        <input type="text" id="first_name" placeholder="Your first name" name="first_name" required="true">
      </div>
      <div class="form-group">
        <label for="last_name">Last name: </label>
        <input type="text" id="last_name" placeholder="Your last name" name="last_name" required="true">
      </div>
      <div class="form-group">
        <label for="email">Email: </label>
        <input type="email" id="email" placeholder="Your email" name="email" required="true">
      </div>
      <div class="form-group">
        <label for="password1">Password: </label>
        <input type="password" id="password1" name="password1" required="true" minlength="8">
      </div>
      <div class="form-group">
        <label for="password2">Confirm password: </label>
        <input type="password" id="password2" name="password2" required="true" minlength="8">
      </div>
      <button type="submit">Submit</button>
    </form>
  </body>
</html>